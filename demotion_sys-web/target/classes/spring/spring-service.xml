<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="nodeService" class="com.jd.cf.demotion_sys.service.impl.NodeServiceImpl">
        <property name="nodeDao" ref="NodeDao"/>
    </bean>
    <bean id="userService" class="com.jd.cf.demotion_sys.service.impl.UserServiceImpl">
        <property name="userDao" ref="UserDao"/>
    </bean>
    <bean id="plateService" class="com.jd.cf.demotion_sys.service.impl.PlateServiceImpl">
        <property name="plateDao" ref="PlateDao"/>
    </bean>
    <bean id="commentService" class="com.jd.cf.demotion_sys.service.impl.CommentServiceImpl">
        <property name="commentDao" ref="CommentDao"/>
    </bean>
</beans>